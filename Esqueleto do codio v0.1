#include <stdio.h>
#include <stdlib.h>

typedef struct No {
    int chave;
    int altura;
    struct No *esq;
    struct No *dir;
} No;

No* novoNo(int valor) {
    No* no = (No*) malloc(sizeof(No));
    no->chave = valor;
    no->altura = 1;
    no->esq = NULL;
    no->dir = NULL;
    return no;
}

No* inserir(No* raiz, int valor) {
    if (raiz == NULL) return novoNo(valor);
    if (valor < raiz->chave)
        raiz->esq = inserir(raiz->esq, valor);
    else if (valor > raiz->chave)
        raiz->dir = inserir(raiz->dir, valor);

    return raiz;
}

void emOrdem(No* raiz) {
    if (raiz != NULL) {
        emOrdem(raiz->esq);
        printf("%d ", raiz->chave);
        emOrdem(raiz->dir);
    }
}

int main() {
    No* raiz = NULL;
    raiz = inserir(raiz, 10);
    raiz = inserir(raiz, 20);
    raiz = inserir(raiz, 30);

    printf("Percurso em ordem: ");
    emOrdem(raiz);
    printf("\n");

    return 0;
}
